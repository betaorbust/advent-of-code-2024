	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug current Jest file",
			"cwd": "${fileDirname}",
			"program": "${workspaceRoot}/node_modules/.bin/jest",
			"args": ["${file}"],
			"console": "integratedTerminal",
			"sourceMaps": true
		}
	]
}
